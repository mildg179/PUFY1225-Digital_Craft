xRad = 200;
yRad = 8;

var evens;

function setup() {
  createCanvas(400, 400);
}

function draw() {
  background("white");
  
  for(var x= xRad/2; x< width; x+=xRad){
    for(var y= yRad/2; y< height; y+=yRad){
      fill("yellow");
      ellipse(x, y, xRad, yRad);
      strokeWeight(5);
      line(400,400,x,y);
      strokeWeight(2);
      
    }
  }
}
